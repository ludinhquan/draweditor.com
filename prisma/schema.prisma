generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model user {
  id          String  @id @map("_id")
  email       String?
  name        String?
  password    String?
  phoneNumber String

  @@unique([email])
  @@unique([phoneNumber])
  @@map("auth.user")
}
